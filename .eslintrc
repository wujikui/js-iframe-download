{
  "env": {
    "browser": true,
    "node": true
  },
  "extends": "airbnb-base",
  "rules": {
    "no-console": "off",
    "no-restricted-globals": "off",
    "no-shadow": "off",
    "no-unused-vars": ["error", { "vars": "all", "args": "none", "ignoreRestSiblings": false }],
    "no-use-before-define": "off",
    "class-methods-use-this": "off",
    "import/no-extraneous-dependencies": ["error", {"devDependencies": ["test_server/**", "*.js"]}]
  }
}
